{
    "name": "AllTube",
    "id": "alltube",
    "packaging_format": 1,
    "description": {
        "en": "Web interface for Youtube-dl",
        "fr": "Interface Web pour Youtube-dl"
    },
    "version": "2.3.0~ynh2",
    "url": "http://alltubedownload.net/",
    "license": "GPL-3.0-only",
    "maintainer": {
        "name": "plopoyop",
        "email": "plopoyop@gmail.com"
    },
    "requirements": {
        "yunohost": ">= 4.1.7"
    },
    "multi_instance": true,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain",
                "example": "example.com"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/alltube",
                "default": "/alltube"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
